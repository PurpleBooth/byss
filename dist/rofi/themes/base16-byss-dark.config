! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #022C64, #022C64, #010114

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #022C64, #BDECF5, #022C64, #022C64, #F7FDFE
rofi.color-active:  #022C64, #BA0AFF, #022C64, #022C64, #BA0AFF
rofi.color-urgent:  #022C64, #FF00A0, #022C64, #022C64, #FF00A0

! Set the desired separator style
rofi.separator-style: solid
