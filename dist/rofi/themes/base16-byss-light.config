! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #daf5fa, #daf5fa, #f7fdfe

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #daf5fa, #0077b6, #daf5fa, #daf5fa, #010228
rofi.color-active:  #daf5fa, #BA0AFF, #daf5fa, #daf5fa, #BA0AFF
rofi.color-urgent:  #daf5fa, #FF00A0, #daf5fa, #daf5fa, #FF00A0

! Set the desired separator style
rofi.separator-style: solid
