! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1a1d19, #1a1d19, #0c100a

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1a1d19, #879385, #1a1d19, #1a1d19, #d5e9d5
rofi.color-active:  #1a1d19, #BA0AFF, #1a1d19, #1a1d19, #BA0AFF
rofi.color-urgent:  #1a1d19, #FF00A0, #1a1d19, #1a1d19, #FF00A0

! Set the desired separator style
rofi.separator-style: solid
